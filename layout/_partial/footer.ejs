<footer id="footer" class="clearfix">

	<div class="social-wrapper">
  	<% if (theme.social) { %>
      <% theme.social.forEach(function(value, key) {
           if (value.url && value.slug) { %>
        <a href="<%= value.url %>" class="social <%= value.slug %>"
          target="_blank" rel="external">
          <span class="icon icon-<%= value.slug %>"></span>
        </a>
      <% } }); %>
    <% } %>
  </div>
	<div>© <%- theme.since %> — <%- year = new Date().getFullYear() %> <%- config.author %></div>
  <div>图标和主题 <a href='https://github.com/stkevintan/hexo-theme-material-flow' class="codename">MaterialFlow</a> 由 <a href="https://dribbble.com/adline">Adline</a> 和 <a href="http://keyin.me/" target="_blank">Kevin Tan</a> 设计 </div>

</footer>
